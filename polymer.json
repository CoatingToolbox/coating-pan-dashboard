    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "node_modules/redux/dist/redux.js",
    "node_modules/d3/build/d3.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  }
