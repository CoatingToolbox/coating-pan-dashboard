{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/elements/pan-summary.html",
    "src/pages/edit-page.html",
    "src/pages/volume-page.html",
    "src/pages/fill-page.html",
    "src/pages/spray-page.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
